# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
end_of_line = lf
insert_final_newline = true
max_line_length = 88
indent_style = space
trim_trailing_whitespace = true

[*.{js, py}]
charset = utf-8

[*.py]
indent_size = 4
max_line_length = 88

[*.{js, html, css, yaml, toml, json, vue}]
indent_size = 2
